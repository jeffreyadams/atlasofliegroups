    let () = real_flag:=true in
    FPP_unitary_hash_general(xlambdas, Uhash, Uparamhash, local_test_GEO_hash_dumb@(
    KGBElt,ratvec,VertexData,VertexData,{vec,vec,}[[FacetVertsIndex]],
    KTypePol_hash, Param_hash))


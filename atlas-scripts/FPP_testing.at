    	      then step := 1
	      fi
    then times = vec: null(N)
    then () = for p@j in ps
    	      do let startp = elapsed_ms()
	      	 then (DEFHTS,) = deform_hts(p,m)
	      	 then () = bools[j]:= is_unitary_to_hts(p, DEFHTS)
		 then () = times[j] := elapsed_ms() - startp
		 then () = if j%step = 0
		      	   then prints("param ",j," tests ",bools[j]," in ",

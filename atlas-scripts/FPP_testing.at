	      od
    then maxLoc = max_loc(times)
    then () = prints("for def to ", m, ", total time = ", print_time_string(elapsed_ms() - start_time),
    	      	     " for ", N, " parameters; #true = ", count(bools));
	      prints("longest = ", print_time_string(times[maxLoc]),
		     " for #", maxLoc)
    in ()


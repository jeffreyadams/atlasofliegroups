lift_std=parameter(lift_std_ext) then 
()=if te_verbose then prints("lift_std=", lift_std) fi in
(p_H,lift_std_ext.parameter,inverse_lift_irr(lift_std,p_H.real_form,M,delta,p_H.infinitesimal_character))




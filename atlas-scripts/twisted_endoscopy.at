set abv_mixed(Param p_H,mat M,RealForm G,mat delta)=(Param,Param,ParamPol):
let lift_std_ext=lift_std(p_H,M,G,delta) then
lift_std=parameter(lift_std_ext) then 
()=if te_verbose then prints("lift_std=", lift_std) fi in
(p_H,lift_std_ext.parameter,inverse_lift_irr_mixed(lift_std,p_H.real_form,M,delta,p_H.infinitesimal_character))


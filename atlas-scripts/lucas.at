set L3=all[3].Levi
set t0=L0.trivial
set t1=L1.trivial
set t2=L2.trivial
set t3=L3.trivial
set z0=parameter(KGB(L0,0),[-2,-1],[0,0])
set z1=parameter(KGB(L1,0),[-2,-1],[0,0])
set z2=parameter(KGB(L2,0),[-2,-1],[0,0])
set z3=parameter(KGB(L3,0),[-2,-1],[0,0])
set D0=theta_induce_irreducible(z0,G)
set D1=theta_induce_irreducible(z1,G)
set D2=theta_induce_irreducible(z2,G)
set D3=theta_induce_irreducible(z3,G)


set L=all[7].Levi

			      then true
			      elif #ps = 1 {otherwise the K-char is wrong!}{)}
				then let still_here = bottom_elim_factors_more(ps[0], pC,
							  datas, pol_hash.index(FVKH[d][j][d+red_shift+1]))
				     then () = if still_here
					       then still_here := bottom_elim_factors_more(ps[0],pC,datasNEW,
							     pol_hash.index(FVKH[d][j][d+red_shift+1]))
					       fi
				     in still_here
				else let still_here = all(for p in ps
						    do bottom_elim_factors_more(p, pC, datas)
						    od)
				     then () = if still_here
					      then still_here := all(for p in ps
							     do @bool: bottom_elim_factors_more(p, pC, datas)
							     od)
					      fi
				    in still_here
				 fi
		  in if not test
		     then newStatuses:= updateNUclassesB(newStatuses, gr, k,"F"); newStatuses[k] := "F"
		     else newStatuses[k] := "T"
		     fi
		  fi {newStatuses[k] = "?"}
		  od {NumClasses}
     then () = if test_verbose
	       then prints("     Total of ", count(for ans in newStatuses do ans[0] = "T" od),
				 " classes are unitary candidates.")
	       fi
     {make new local face list using only faces not already proven nonunitary}
     then FVKHnew = for @m in FVKH do [FaceVertsKHash]: [] od
	  then () = for C@m in eq do if not newStatuses[m][0] = "F"
				     then for k in C
					  do let (d,j) = coords(FVKH,k)
					     in FVKHnew[d] #:= FVKH[d][j]
					  od
				     fi
				  od
     then time = elapsed_ms() - start
     then () = if #Ds > 1 and fund_face_verbose
	       then prints("at x = ",x.number,", lambda = ",lambda,"# datas = ",#datas,", #Ds = ",#Ds)
	       fi
     then () = if time_verbose then prints("     ", print_time_string(time),
			       " for local_testK_hash_bottom_layer_more_step; number of tests = ",
			       unitary_test_counter.use_count() - start_count) fi
     in  if sort_LFD_flag
	 then sort(x.real_form, Lvd, FVKHnew)
	 else FVKHnew
	 fi

set local_testK_hash_bottom_layer_more_step(KGBElt x, ratvec lambda, LocalVertexData Lvd,
    [[FaceVertsKHash]] FVKH, [[int]] eq, [[int]] gr, [vec] classListByFace, KGPElt Qmin,
     {[KGPElt] maxQs,} [QKL_bottom_data] datas, [int] Ds{, KTypePol MU}, KTypePol_hash pol_hash,
     Param_hash Uhash) = [[FaceVertsKHash]]:
     local_testK_hash_bottom_layer_more_step(x, lambda, Lvd, FVKH, eq, gr, classListByFace,
     Qmin, {maxQs,} datas, Ds, {MU,} pol_hash, Uhash, is_unitary_hash_big_SIMPLE@Param)

set ParamsByDim = [[Param]]: for d:16 do [] od
set LFDKHByDim = [[FaceVertsKHash]]: for d:16 do [] od

{takes the unitary Param's listed in LFDKHByDim and computes the K-character
 number in pol_hash}
set KcharsByDim() = [[int]]:
    for j:16 do for v in LFDKHByDim [j] do v[j+1] od od

{lists all distinct K-characters (by number in pol_hash)}
set AllKchars() = [int]:
    KcharsByDim().##.sort_u

set NumKchars() = int:
    #AllKchars()

set more_verbose =false

{if true, hash_down runs through faces small to large, hoping for nonunitarity}
{set hash_down = true}

set final_verbose = false

{ Input is candidate faces, and graph info. Output is the tester-unitary ones.
 use and update a complete list Uhash of known tester-unitary faces.
 try to compute and systematically use partial order on faces}
set local_test_hash_old(KGBElt x, ratvec lambda, LocalVertexData Lvd,
    [[LocalFaceVertsKHash]] LFVH, [[int]] eq, [[int]] gr, [vec] classListByFace,
    (Param->bool) tester, Param_hash Uhash, (Param->bool) fancy_tester) = [Param]:
    let NumClasses = #eq, printInterval = int: 1, start = elapsed_ms()
    then newStatuses = for m:NumClasses do "?" od
    then start_count = unitary_test_counter.use_count(), H0 = height(parameter(x,lambda,0*lambda))
    then () = if fund_face_verbose and NumClasses > 100
	      then prints("start old local_test, ",NumClasses,
	      " equiv classes of faces")
	      fi
    {then start_by_dim = vec: for list in LFVH do #list od}
    then () = if KU_flag
	      then let() = KUKU_flag:=false
		   then knownUnitaries = known_unitaries(Uhash, x, lambda, Lvd, LFVH, fancy_tester)
		   {then () = prints(knownUnitaries)}
		   then () = for list@d in knownUnitaries
			     do for j in list
				do let k = classListByFace[d][j]
				   in newStatuses:= downdateUclassesB(newStatuses, gr, k, "T");
				      newStatuses[k] := "T"
				od
			     od
		   in KUKU_flag:=true
	       fi
     then () = if KNU_flag
	       then let knownNonUnitaries = known_non_unitaries(x, lambda, Lvd, LFVH)
	       in for list@d in knownNonUnitaries
		  do for j in list
		     do let k = classListByFace[d][j]
			in newStatuses:= updateNUclassesB(newStatuses, gr, k, "F");
			   newStatuses[k] := "F"
		      od {j in list}
		   od {list@d}
		fi
    then known_true_count = 0, known_false_count = 0, unknown_ms = [int]:[]
    then () = for s@m in newStatuses
	      do if s[0] = "T" then known_true_count +:= 1
		 elif s[0] = "F" then known_false_count +:=1
		 else unknown_ms #:=m
		 fi
	      od
    then () = if test_verbose
	      then prints("     Examining ",NumClasses," classes in local_test_hash_old; ",
	      known_true_count," are known unitary, ", known_false_count, " known nonunitary.")
	      fi
    then () = if final_verbose and #unknown_ms > 0
	      then prints("representatives of unknown classes:");
		   for m in unknown_ms
		   do let C = eq[m]
		      then N = if final_wiggle_flag and C~[0] >= #LFVH[0]
			       then C~[0]
			       else C[0]
			       fi
		      then (d,j) = coords(LFVH,N)
		      then verts = vec: LFVH[d][j][:d+1]
		      then gamma = face_bary(Lvd,verts)
		      then pC = parameter(x, lambda, gamma)
		      in prints("   ",pC, " for class ",m," of size ",#C)
		   od
	      fi {final_verbose}
    then () = for m@k in unknown_ms {work from small faces toward big}
	     ~do let C = eq[m]
		 then N = if final_wiggle_flag and C~[0] >= #LFVH[0]
			  then C~[0]
			  else C[0]
			  fi
		 then (d,j) = coords(LFVH,N)
		 then verts = vec: LFVH[d][j][:d+1]
		 then gamma = face_bary(Lvd,verts)
		 then pC = parameter(x, lambda, gamma)
		 then ps = monomials(pC)
		 then () = if final_wiggle_flag and d>0
			   then let short_verts = for i:d+1
						      do verts[:i]##verts[i+1:]
						      od {list of vertex sets for faces}
				    then CF = character_formula(1*pC)
				    then pdef1 = for sv in short_verts
						      do monomials(composition_series(wiggle(CF,
								face_bary(Lvd,sv))))
						      od.##
					 in if any(for q in pdef1
						   do height(q) > H0 and not tester(q)
						   od)
					    then newStatuses:= updateNUclassesB(newStatuses, gr, m);
						 newStatuses[m] := "F"
					    fi {any q}
				fi {final_wiggle_flag}
		 then () = if newStatuses[m][0] = "?" and all(for p in ps
				   do @bool: Uhash.lookup(p).>= or (let ans = tester(p)
								   in if not ans
								      then if more_after_flag
									   then more_bad_hash.match(p)
									   elif min_after_flag
									   then bad_hash.match(p)
									   fi
								       fi; ans)
				   od)
			    then newStatuses[m]:="T"
			    else newStatuses:= updateNUclassesB(newStatuses, gr, m);
				     newStatuses[m] := "F"
			    fi
		then kopp = (#unknown_ms) - k - 1
		then () = if kopp < k
		     then let M = unknown_ms[kopp]
			  then () = if fund_face_verbose and kopp%printInterval = 0
				   then let unk = #q_nums(newStatuses[M:m])
					in prints("testing equivalence classes #",M, " and #",m,
				   "; so far ",unitary_test_counter.use_count() - start_count
				   ," 'is_unitary' tests; ",unk," classes still ? at ",
					     print_time_string(elapsed_ms() - start))
				   ; printInterval := (3*printInterval)\2 + 1
				   fi {fund_face_verbose}
			   in if newStatuses[M][0] = "?" {NEWU}
			      then let C=eq[M]
				   then N = if final_wiggle_flag and C~[0] >= #LFVH[0]
					    then C~[0]
					    else C[0]
					    fi
				   then (d,j) = coords(LFVH,N) {always easier to test FIRST term in equiv class?}
				   then verts = vec: LFVH[d][j][:d+1]
				   then pC = parameter(x, lambda, face_bary(Lvd, verts))
				     {first_param makes trouble in UParams list!}
		       then ps = monomials(pC)
		       then () = if final_wiggle_flag and d>0
					  then let short_verts = for i:d+1
						      do verts[:i]##verts[i+1:]
						      od {list of vertex sets for faces}
					       then CF = character_formula(1*pC)
					       then pdef1 = for sv in short_verts
						      do monomials(composition_series(wiggle(CF,
								face_bary(Lvd,sv))))
						      od.##
					 in if any(for q in pdef1
						   do height(q) > H0 and not tester(q)
						   od)
					    then newStatuses[M] := "F"
					    fi {any q}
				    fi {final_wiggle_flag}
			in if newStatuses[M][0] = "?" and all(for p in ps
					  do @bool: Uhash.lookup(p).>= or (let ans = tester(p)
									   in if not ans
									      then if more_after_flag
										   then more_bad_hash.match(p)
										   elif min_after_flag
										   then bad_hash.match(p)
										   fi {more_after}
									      fi{not ans}
									      ; ans)
					od) {all for p in ps}
			    then newStatuses := downdateUclassesB(newStatuses, gr, M,"T");
				 newStatuses[M] := "T"
			    else newStatuses[M] := "F"
			    fi {all for p in ps}
		       fi {NEWU} {list of unitary Params}
		fi {kopp < k}
	   in ()
	  od {m:NumClasses}
     {while(known_true_count + known_false_count < NumClasses)
     do if known_true_count>known_false_count}
     then unitary_by_dim = vec: null(#LFVH)
     then UParams = [Param]:[]
     then () = for C@k in eq
	       do if newStatuses[k][0] = "T"
		  then for i in C
		       do let (d,j) = coords(LFVH,i)
			  then moreverts = vec: LFVH[d][j][:d+1]
			  then () = unitary_by_dim[d]+:=1
			  then q = parameter(x,lambda, face_bary(Lvd,moreverts))
			  in UParams #:= q;
			     if long_out_flag
			     then ParamsByDim[d] #:=q;
				  LFDKHByDim[d]#:=LFVH[d][j]
			     fi;
			     for q1 in monomials(finalize(q))
			     do Uhash.match(q1);
				if deform_flag
				then for q1d in facet_deformed_new(q1)
				     do Uhash.match(q1d)
				     od
				fi {deform_flag}
			     od {q1 in monomials}
		       od {i in C}
		   fi {newStatuses[k]}
		od
     then () = if test_verbose
	       then prints("     Total of ", count(for ans in newStatuses do ans[0] = "T" od),
				 " classes are unitary.")
	       fi
    then () = if test_verbose or test_slightly_verbose
	      then prints("unitary faces by dim = ",unitary_by_dim, test_string);
		   prints("number of tests = ",unitary_test_counter.use_count() - start_count, ", in ",
			print_time_string(elapsed_ms() - start))
	      fi
    in  UParams

{ Input is candidate faces, and graph info. Output is the tester-unitary ones.
 use and update a complete list Uhash of known tester-unitary faces.
 try to compute and systematically use partial order on faces, working from vertices up}
set local_test_hash_up(
    KGBElt x,
    ratvec lambda,
    LocalVertexData Lvd,
    [[LocalFaceVertsKHash]] LFVH,
    [[int]] eq, { equivalence classes of local faces }
    [[int]] gr, { graph structure on equivalence classes }
    [vec] classListByFace,
    (Param->bool) tester,
    Param_hash Uhash,
    (Param->bool) fancy_tester
    ) = [Param]:
begin
    let NumClasses = #eq, printInterval = int: 1, start = elapsed_ms()
    then newStatuses = for m:NumClasses do "?" od
    ,    H0 = height(parameter(x,lambda,0*lambda))
    then start_count = unitary_test_counter.use_count()
in
    if fund_face_verbose and NumClasses > 100
    then prints("start up local_test, ",NumClasses,
    " equiv classes of faces")
    fi
    {then start_by_dim = vec: for list in LFVH do #list od}
;   if KU_flag
    then KUKU_flag:=false
    ;	let knownUnitaries = [vec]: known_unitaries(Uhash, x, lambda, Lvd, LFVH, fancy_tester)
	{then () = prints(knownUnitaries)}
    in for list@d in knownUnitaries
       do for j in list
	  do let k = classListByFace[d][j]
	     in newStatuses:= downdateUclassesB(newStatuses, gr, k, "T"); { make "T" below cllas k }
		newStatuses[k] := "T"
	  od
       od
    ;  KUKU_flag:=true
    fi
;   if KNU_flag
    then let knownNonUnitaries = known_non_unitaries(x, lambda, Lvd, LFVH)
    in for list@d in knownNonUnitaries
       do for j in list
	  do let k = classListByFace[d][j]
	     in newStatuses:= updateNUclassesB(newStatuses, gr, k, "F");
		newStatuses[k] := "F"
	   od {j in list}
	od {list@d}
     fi
;   let known_true_count = for s in newStatuses do s[0]="T" od.count
    , known_false_count =  for s in newStatuses do s[0]="F" od.count
    , unknown_ms = [int]: for s@m in newStatuses if s[0]="?" do m fi od
in  if test_verbose
    then prints("     Examining ",NumClasses," classes in local_test_hash_up; ",
    known_true_count," are known unitary, ", known_false_count, " known nonunitary.")
    fi
;   if final_verbose and #unknown_ms > 0
    then prints("representatives of unknown classes:");
	 for m in unknown_ms
	 do let C = eq[m]
	    then N = if final_wiggle_flag and C~[0] >= #LFVH[0]
		     then C~[0]
		     else C[0]
		     fi
	    then (d,j) = coords(LFVH,N)
	    then verts = vec: LFVH[d][j][:d+1]
	    then gamma = face_bary(Lvd,verts)
	    then pC = parameter(x, lambda, gamma)
	    in prints("   ",pC, " for class ",m," of size ",#C)
	 od
    fi {final_verbose}
;   for m@j in unknown_ms {work from small faces toward big}
   ~do if fund_face_verbose and (#unknown_ms - 1 - j)%printInterval = 0
       then let unk = #q_nums(newStatuses[:m])
	    in prints("testing ",j,"th unknown equivalence class #",m,
			"; so far ",unitary_test_counter.use_count() - start_count
			," 'is_unitary' tests; ",unk," classes still ? at ",
				  print_time_string(elapsed_ms() - start))
			; printInterval := (3*printInterval)\2 + 1
       fi {fund_face_verbose}
    ;
       let C = eq[m]
       then N = if final_wiggle_flag and C~[0] >= #LFVH[0]
		then C~[0]
		else C[0]
		fi
       then (d,j) = coords(LFVH,N)
       then verts = vec: LFVH[d][j][:d+1] { extract the "vertices" part of the |vec| }
       then gamma = face_bary(Lvd,verts)
       then pC = parameter(x, lambda, gamma)
       {then () = prints("now testing pC = ",pC,", new_statuses[m] = ",newStatuses[m])}
       then ps = [Param]: monomials(pC)
    in if final_wiggle_flag and d>0
       then let short_verts = for i:d+1 do delete(verts,i) od {list of vertex sets for faces}
	    then CF = character_formula(1*pC)
	    then pdef1 = for sv in short_verts
			 do monomials(composition_series(wiggle(CF,
					   face_bary(Lvd,sv))))
			 od.##
	 in if any(for q in pdef1 do @: height(q) > H0 and not tester(q) od)
	    then {prints("parameter ",pC," tests nonunitary by wiggle, verts = ",verts);}
		 newStatuses:= updateNUclassesB(newStatuses, gr, m)
	    ;	 newStatuses[m] := "F"
	    { nothing more will happen in this loop iteration }
	    fi {any q}
	fi {final_wiggle_flag}
      {then () = prints("got past wiggle with newStatuses[m] = ",newStatuses[m])}
     ;  if newStatuses[m][0] = "?"
	then
	   if for p in ps
	      do @bool: Uhash.lookup(p)>=0 or tester(p) or
		 ( false { in remaining case make |all| fail, but do: }
		 next
		   if more_after_flag
		   then more_bad_hash.match(p)
		   elif min_after_flag
		   then bad_hash.match(p)
		   fi
		 )
	      od.all
	   then {prints("class ",m," tested true");}
		newStatuses[m]:="T"
	   else {prints("class ",m," tested false");}
	      newStatuses:= updateNUclassesB(newStatuses, gr, m)
	   ;  newStatuses[m] := "F"
	   fi {all (for p in ps)}
      {else prints("sorry, NOT testing since newStatuses[m] = ",newStatuses[m])}
       fi {newStatuses[m] = "?"}
    od {m in unknown_ms}
;   let unitary_by_dim = vec: null(#LFVH)
    ,   UParams = [Param]:[]
   {then () = prints(for s@k in newStatuses do prints(k,"  ",s) od)}
in  for C@k in eq
    do if newStatuses[k][0] = "T"
       then for i in C
	    do let (d,j) = coords(LFVH,i)
	       then moreverts = vec: LFVH[d][j][:d+1]
	       then () = unitary_by_dim[d]+:=1
	       then q = parameter(x,lambda, face_bary(Lvd,moreverts))
	    in UParams #:= q
	    ;  if long_out_flag
	       then ParamsByDim[d] #:= q;LFDKHByDim[d]  #:= LFVH[d][j]
	       fi
	    ;  for @q1 in finalize(q)
	       do Uhash.match(q1);
		  if deform_flag
		  then for q1d in facet_deformed_new(q1) do Uhash.match(q1d) od
		  fi {deform_flag}
	       od {q1 in monomials}
	    od {i in C}
	fi {newStatuses[k]}
    od
;   if test_verbose
    then prints("     Total of ", count(for ans in newStatuses do ans[0] = "T" od),
		      " classes are unitary.")
    fi
;   if test_verbose or test_slightly_verbose
    then prints("unitary faces by dim = ",unitary_by_dim, test_string);
	 prints("number of tests = ",unitary_test_counter.use_count() - start_count, ", in ",
	      print_time_string(elapsed_ms() - start))
    fi
;   UParams
end

{ Input is candidate faces, and graph info. Output is the tester-unitary ones.
 use and update a complete list Uhash of known tester-unitary faces.
 try to compute and systematically use partial order on faces, working from big faces down}
{this version appears to be MUCH slower than up (factor of 4)}
{set local_test_hash_down(KGBElt x, ratvec lambda, LocalVertexData Lvd,

set local_test_GEO2(KGBElt x, ratvec lambda) = [Param]:
    let Uhash = big_unitary_hash.uhash(x.real_form) {make_Param_hash()}, pol_hash = make_KTypePol_hash(), G = x.real_form
    {then vd = FPP_vertex_data(G)}

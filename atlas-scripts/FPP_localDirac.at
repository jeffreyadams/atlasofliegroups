			   then prints("unitary face counts to dim ",D+1,
				": ", for list in LFDKH do #list od) fi
                 then ()= D:=D+1
                 in ()
     	      od
	      in ()
	      fi{quick_flag}
    {now LFDKH is everything unitary to ht one_ht}
    then unitary_by_dim = vec: for L in LFDKH do #L od
    then () = if test_verbose then { prints("unitary faces by dim = ",

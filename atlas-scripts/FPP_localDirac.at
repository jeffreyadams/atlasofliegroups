			       fi
		     in p
                  od
                od.##
    then time = elapsed_ms() - start
    then () = if time_verbose
	      then prints("     ", print_time_string(time),
	      " for UParams calculation ", test_string, new_line)
	      fi
   in UParams {local_test_GEO_hash_real_dumb_end}


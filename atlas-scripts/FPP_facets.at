	       then jump = first(for N@k in LF[0][j+1:] do Lvd.list[N[0]]*trc > nextht od)
	       then () = prints("j = ",j," jump = ",jump)
	       then () = total_checks := if jump>=1 then total_checks+jump-1
	       	       	 	      	 else total_checks
					 fi
	       in for N@k in LF[0][j+1:j+jump]
	       	  do let w = Lvd.list[N[0]]
	       	     then edge = (v-w), bary = (v+w)/2
		     in if REH.lookup(dominant(G,edge)).>= 
	       	     	then let ends = local_vertices(parameter(x,lambda,bary), vd, Lvd, Perm)
	       	             in if #ends = 2 and
	             	       	   ((ends[0] = v and ends[1] = w) or
	       	     	            (ends[0] = w and ends[1] = v))

(   assert(#LF = 1,"localFD_01new takes only local VERTEX list");
    if facet_verbose
    then prints(new_line, "start localFD_01new")
    fi;
    let G = x.real_form, start=elapsed_ms()
    then (Affd, M, j0) = cofolded(G), total_checks = 0
    then Edges = [vec]:
    	 if #LF[0] <= 1 then []
	 else
	 let () = LF := sort(G,Lvd,LF,0), trc = G.two_rho_check,
	     fvs =fundamental_vertices(G)
	 then RVH = make_ratvec_hash()
	 then REH = make_ratvec_hash()
	 then vert_cands = [ratvec]: fvs ## for v@j in fvs
	      		   	     	    do for k:#fvs -j - 1 from j+1
				     	       do (v+fvs[k])/2
				   	       od {midpts starting at v}
					    od.## {all }
	 then () =  for v in vert_cands do RVH.match(dominant(G,v)) od
	 then N = RVH.size()
	 then () = prints("found ",N," candidate local verts")
	 then () = for j:N-1
	      do for k:N-j-1 from j+1
	      	 do let bary = (RVH.index(j) - RVH.index(k))
		    in REH.match(dominant(G, bary))
		 od
	      od
	 then () = prints("found ",REH.size(), " candidate local edges")
	 then maxedge = max (for j:REH.size() do REH.index(j)*trc od)
    	 in [vec]:
	    for M@j in LF[0]

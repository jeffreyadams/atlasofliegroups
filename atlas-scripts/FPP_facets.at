	      then prints("     ", print_time_string(time),
	      " for localFD_KHash001 to heights ",h, ", ",hK) fi
    in if(facet_verbose) then prints("After localFD_KHash001, vertex count is ",
    #FVK0) fi;
       sort(x.real_form, Lvd, [FVK0], 2)

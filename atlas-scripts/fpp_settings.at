{load this file last so these setting override the defaults}

FPP_report_flag:=true
every_lambda_deets_flag := true
every_KGB_flag:= true
fund_face_verbose:=true
seat_belt_on:=false

set to_ht_factors =[1,2,3,4,5,6]
set u_factors = [1,2,3,4,5,6]
set prefer_u_factors = [2/3,1,4/3,5/3,2,7/3]

{box(6,3) gives [0,0,0],[0,0,1],...,[0,0,5],[0,1,0],...[5,5,5]}
set set_factors(int n)=
let triple=box(6,3)[n]
in (to_ht_factors[triple[0]],u_factors[triple[1]],prefer_u_factors[triple[2]]) 

set springer_by_signature_E7(ComplexNilpotent O)=
assert(O.root_datum.Lie_type=Lie_type("E7"),"Not type E7");
let d=O.diagram then
index=first(for i:#springer_by_signature_lookup_E7
 do let (v,)=springer_by_signature_lookup_E7[i] in v=d od) in
 let (,sig)=springer_by_signature_lookup_E7[index] in sig

set springer_E7(CharacterTable ct,ComplexNilpotent O)=int:
let characters=ct.characters then
(sig_0,sig_1)=springer_by_signature_E7(O) in
first(for v@i in characters do
v[0]=sig_0 and ct.degrees[i]=sig_1 od)


set springer_table_E7(CharacterTable ct)=
let rd=ct.root_datum then
orbits=rd.orbits then
dual_map(ComplexNilpotent O)=ComplexNilpotent: dual_map_E7(O) then
dual_map_i(ComplexNilpotent O)=ComplexNilpotent: dual_map_E7(O) then
springer(ComplexNilpotent O)=springer_E7(ct,O) in
(ct,orbits,dual_map,dual_map_i,springer)

set dual_map_E7_diagram(vec v)=vec:
let i=first(for i:#dual_diagram_map_E7 do let(x,)=dual_diagram_map_E7[i] in x=v od)
then (,y)=dual_diagram_map_E7[i] in y

set dual_map_E7(ComplexNilpotent O)=ComplexNilpotent:
complex_nilpotent_from_diagram(O.root_datum,dual_map_E7_diagram(O.diagram))

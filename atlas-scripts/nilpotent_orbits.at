let g=generators_of_component_group(O)
then orders=## for (a,H,b,c,d,S) in g do for v in S do order_in_A(v,O.root_datum,H) od od in
(O,sort(orders),for (L,H,b,c,d,S) in g do (L,S) od)
fi

